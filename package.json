{
  "name": "create-h3ravel",
  "type": "module",
  "version": "0.6.1",
  "description": "Scaffold a new H3ravel applications using templates and starter kits",
  "main": "build/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "create-h3ravel": "bin/run.js"
  },
  "exports": {
    ".": "./bin/run.js"
  },
  "keywords": [
    "h3ravel",
    "create-h3ravel"
  ],
  "scripts": {
    "quick:test": "pnpm vitest",
    "test": "pnpm run quick:test",
    "runner": "tsx --experimental-specifier-resolution=node src/run.ts",
    "build": "tsdown",
    "version": "pnpm run build",
    "unlink": "npm unlink -g create-h3ravel",
    "release": "pnpm build && pnpm changeset version && pnpm changeset publish",
    "publish-it": "pnpm build && pnpm publish --tag latest"
  },
  "files": [
    "bin",
    "bin/run.d.ts",
    "bin/run.js"
  ],
  "dependencies": {
    "@antfu/install-pkg": "1.1.0",
    "@h3ravel/musket": "^0.2.0",
    "@h3ravel/shared": "^0.22.2",
    "@h3ravel/support": "^0.14.1",
    "@inquirer/core": "^10.2.2",
    "giget": "^2.0.0",
    "inquirer": "^12.9.6",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@swc/core": "^1.6.1",
    "@types/node": "^20.14.5",
    "eslint": "^9.32.0",
    "ts-node": "^10.9.2",
    "tsdown": "^0.15.4",
    "tsx": "^4.20.3",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.14.0"
}