{
  "name": "create-h3ravel",
  "type": "module",
  "version": "0.2.0",
  "description": "Scaffold a new H3ravel applications using templates and starter kits",
  "main": "build/index.js",
  "bin": {
    "create-h3ravel": "bin/run.js"
  },
  "exports": {
    ".": "./bin/run.js"
  },
  "keywords": [
    "h3ravel",
    "create-h3ravel"
  ],
  "scripts": {
    "quick:test": "pnpm jest",
    "test": "pnpm run quick:test",
    "runner": "tsx --experimental-specifier-resolution=node src/run.ts",
    "build": "tsup && cp logo.txt bin/logo.txt",
    "version": "pnpm run build",
    "unlink": "npm unlink -g create-h3ravel",
    "release": "pnpm build && pnpm changeset version && pnpm changeset publish",
    "publish": "pnpm build && pnpm -r publish --access public"
  },
  "files": [
    "bin",
    "bin/run.d.ts",
    "bin/run.js"
  ],
  "dependencies": {
    "@antfu/install-pkg": "1.1.0",
    "@h3ravel/shared": "^0.18.1",
    "@h3ravel/support": "^0.10.0",
    "@inquirer/core": "^10.2.2",
    "commander": "^14.0.0",
    "giget": "^2.0.0",
    "inquirer": "^12.9.6",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@swc/core": "^1.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.5",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@10.14.0"
}